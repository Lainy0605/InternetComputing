<!--pages/my/successHabits/successHabits.wxml-->
<view class="head">您已经成功培养 {{nums}} 个习惯</view>
<view class="display-area">
    <view class="medal" wx:for="{{successHabits}}" wx:key="item">
        <image src='../../../images/medal-without-star.png' style="height: 264rpx; width: 244rpx;"></image>
        <span class="habitsTitle">
            <text style="height: auto; text-align: center;">{{item.name}}</text>
        </span>
        <view class="time">{{item.endTime}}</view>
    </view>
</view>