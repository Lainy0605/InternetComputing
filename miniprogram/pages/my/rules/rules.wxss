Page {
    background-color: white;
}
.page{
    border-style: dotted;
    border-color: #EBA13E;
    background-color: #e8f7b942;
    margin: 15rpx 15rpx 15rpx 15rpx;
    padding-right: 5rpx;
}
.title{
    display: flex;
    font-size: 35rpx;
    align-content: center;
    justify-content: center;
    font-weight: 700;
}
.bigtitle{
    font-size: 25rpx;
    margin-left: 10rpx;
    margin-bottom: 5rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
}
.table{
    font-size: 25rpx;
    margin-left: 35rpx;
    margin-bottom: 5rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.middletitle{
    font-size: 25rpx;
    margin-left: 35rpx;
    margin-bottom: 5rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
}
.content{
    margin-left: 10rpx;
    margin-bottom: 5rpx;
}
.bottom{
    position: relative;
    width: 100%;
    height: 100rpx;
}
.name{
    position: absolute;
    right: 45rpx;   
    font-weight: 600;
    top: 10rpx; 
}
.time{
    position: absolute;
    right: 55rpx;   
    font-weight: 600;
    top: 50rpx;   
}
.su-table{
    margin: 0 auto;
    display: table;
    border-collapse: collapse;
    overflow-x: auto;
    word-break: keep-all;
    white-space: nowrap;
    width: 90%;
    border-radius: 20rpx;
}
.su-tr{
    display: table-row;
}
.su-th{
    display: table-cell;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1rpx solid #DDDDDD;
    font-weight: 600;
}
.su-td{
    display: table-cell;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1rpx solid #DDDDDD;
}
.xuhao{
    width: auto;
}
